<h1>Listing blogposts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Written at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @blogposts.each do |blogpost| %>
  <tr>
    <td><%= blogpost.title %></td>
    <td><%= blogpost.content %></td>
    <td><%= blogpost.written_at %></td>
    <td><%= link_to 'Show', blogpost %></td>
    <td><%= link_to 'Edit', edit_blogpost_path(blogpost) %></td>
    <td><%= link_to 'Destroy', blogpost, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Blogpost', new_blogpost_path %>
